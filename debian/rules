#!/usr/bin/make -f
# debian/rules for makenl

%:
	dh $@

override_dh_auto_clean:
	$(MAKE) -C src -f makefile.linux clean
	#$(MAKE) -C docs -f Makefile clean

override_dh_auto_build:
	$(MAKE) -C src -f makefile.linux
	#$(MAKE) -C docs -f Makefile

override_dh_auto_install:
	dh_auto_install --destdir=debian/makenl

override_dh_installdocs:
	dh_installdocs --link-doc=makenl

override_dh_strip:
	dh_strip --dbg-package=makenl-dbg

.PHONY: override_dh_strip